                <div class="edit-table">
                    <div class="row">
                        <div class="col-lg-12 col-sm-12">
                            <div class="card m-b-30">
                                <div class="card-body">
                                    <h5 class="header-title">Edit Your Coupons Here</h5>
                                    {{!-- <p class="text-muted">Add toolbar column with edit and delete buttons.</p> --}}
                                    <div class="">

                                        <table class="table" id="my-table">
                                            <thead>
                                                <tr>
                                                    <th>S-No:</th>
                                                    <th>Coupon-ID</th>
                                                    <th>Coupon-name</th>
                                                    <th>Expiry Date</th>
                                                    <th>Percentage</th>
                                                    <th>MIN Amount</th>
                                                    <th>MAX Amount</th>
                                                    <th>Status</th>
                                                    <th>Action</th>

                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    {{#each coupen}}
                                                    <td>{{inc @index}}</td>
                                                    <td>{{this.code}}</td>
                                                    <td>{{this.name}}</td>
                                                    <td>{{this.endingdate}}</td>
                                                    <td>{{this.value}}</td>

                                                    <td>{{this.minAmount}}</td>
                                                     <td>{{this.maxAmount}}</td>

                                                   <td>{{this.status}}</td>
                                                     <td>

                                                        <div class="btn-group">
                                                            <a href=""
                                                                class="btn btn-dark  text-white  btn-rounded text-align-center mt-4 pt-3"
                                                                data-placement="top" data-toggle="tooltip"
                                                                data-original-title="DELETE">
                                                                <p class="block">DELETE</p>
                                                            </a>
                                                            <a href=""
                                                                class=" btn btn-outline-dark   text-dark btn-rounded text-align-center    mt-4 pt-3"
                                                                data-placement="top" data-toggle="tooltip"
                                                                data-original-title="UPDATE">
                                                                <p class="unblock">UPDATE</p>
                                                            </a>
                                                        </div>


                                                    </td>
                                                </tr>
                                                {{/each}}

